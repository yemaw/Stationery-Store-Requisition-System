<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.logicuniversityemployee"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="11"
        android:targetSdkVersion="17" />
    <uses-permission android:name="android.permission.INTERNET"/>

    <application
        android:allowBackup="true"
        android:icon="@drawable/icon_57"
        android:label="@string/app_name"
        android:theme="@style/Theme.Example" >
        <activity
            android:name="com.example.logicuniversityemployee.SplashScreenActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity  android:name="com.example.logicuniversityemployee.MainActivity"></activity>
        <activity  android:name="com.example.logicuniversityemployee.ApprovedRequisitionActivity"></activity>
        <activity  android:name="com.example.logicuniversityemployee.DistributedRequisitionActivity"></activity>
        <activity  android:name="com.example.logicuniversityemployee.PendingRequisitionActivity"></activity>
        <activity  android:name="com.example.logicuniversityemployee.ProcessingRequisitionActivity"></activity>
        <activity  android:name="com.example.logicuniversityemployee.RejectedRequisitionActivity"></activity>
        <activity  android:name="com.example.logicuniversityemployee.LoginActivity"></activity>
    </application>

</manifest>
